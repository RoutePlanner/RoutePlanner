<head>
    <meta http-equiv="Content-Type" content="text/html; charset=gb2312" />
    <title>RoutePlanner </title>
    <link href="@Url.Content("~/Content/Index.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/js/Index.js")" type="text/javascript"></script>
</head>

<body>
    <div id="container">
        <div id="header">Header</div>
        <div id="topsidebar">
            <div>
                <div>Home</div>
            </div>
        </div>
        <div id="mainContent">
            <div id="sidebar">
                <div class="planner">
                    <ul class="planner_title planner_title_bg1" id="plannertitle">
                        <li><a href="#" target="_blank" onmouseover="javascript:plannerq(1)">规划路线</a></li>
                        <li><a href="#" target="_blank" onmouseover="javascript:plannerq(2)">找地方</a></li>
                    </ul>
                    <div class="planner_con" id="planner_con1" style="display: block;">
                        <div>出发：<input type="text" id="StartInput" /></div>
                        <div>到达：<input type="text" id="EndInput" /></div>
                    </div>
                    <div class="planner_con" id="planner_con2" style="display: none;">
                        地址：<input type="text" id="LocationSearchInput" />
                    </div>
                </div>
            </div>
            <div id="content">antmap</div>
        </div>
    </div>
</body>
